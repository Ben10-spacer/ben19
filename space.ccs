let cor;
let positionX;
let positinY;

function sletup() {
  createCanvas(400, 400);
  background(195);
  cor = color(random(0,255), random(0,255), random(0,255));
}

function draw() {
  positionX = mouseX;
   positionY = mouseY;
  if(mouseIsPressed) {
    circle (positionX,positionY,50);
    fill (cor);
    stroke (cor);
    cor = color(random (0,255), random (0,255),random (0,255));
            
  }
}
    
  

  
